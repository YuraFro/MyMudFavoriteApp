@page "/game"
@page "/game/{id:long}"
@inject IGameServiceClient GameService

@code {
    [Parameter]
    public long? Id { get; set; }

    Game game = new Game() { GameStudio = new GameStudio() };

    protected override async Task OnParametersSetAsync()
    {
        if (Id == null)
        {

        }
        else
        {
            game = await GameService.GetSingleGame((long)Id);
        }
    }
}
